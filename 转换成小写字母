[C#]

public class Solution {
    public string ToLowerCase(string str) {
          string s = null;
        int t = 0;
        for (int i = 0; i < str.Length; i++)
        {
            t = (int)(str[i]);
            if (t >= 65 && t < 90)
                s += (char)((t + 32));
            else
                s += str[i];
        }

        return s;
    }
}

public class Solution {
    public string ToLowerCase(string str) {
        return str.ToLower();
    }
}
